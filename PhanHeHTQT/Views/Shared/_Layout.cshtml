                        v.classList.remove("submenu-closed");
                        v.classList.remove("submenu-open");
                    } else v.classList.add("active");
                });
                let action_ = document.querySelector(`[action_name="${control}_${action}"]`);
                action_.classList.add("active");
            } catch (ex) {

            }
        </script>
        @await RenderSectionAsync("Scripts", required: false)

    </body>

    </html>



